{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/ServerlessDotnetApi.sln"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ServerlessDotnetApi.sln"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn":["clean"]
        }
    ]
}